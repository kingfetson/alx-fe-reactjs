{
  "version": 2,
  "builds": [
    {
      "src": "vite.config.js",
      "use": "@vercel/static-build",
      "config": {}
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "env": {
    "VITE_APP_GITHUB_API_URL": "https://api.github.com"
  },
  "ignoreCommand": "echo 'Skipping files listed in .gitignore'"
}
